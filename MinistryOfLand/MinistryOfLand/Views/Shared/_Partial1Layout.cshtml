    <link href="https://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet">
    <link rel="icon" href="~/Content/Image/favicon.ico" type="image/gif" sizes="16x16">
    <script src="js/modernizr.js"></script> <!-- Modernizr -->
