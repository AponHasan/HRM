@model MinistryOfLand.Models.UserAccount
@{
    ViewBag.Title = "DetailsUser";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>DetailsUser</h2>

<div>
    <h4>User</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            User Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.username)
        </dd>

        <dt>
            Password
        </dt>

        <dd>
            @Html.DisplayFor(model => model.password)
        </dd>

        <dt>
            Mail
        </dt>

        <dd>
            @Html.DisplayFor(model => model.email)
        </dd>

        <dt>
            User Type
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserType)
        </dd>
    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "EditUser", new { id = Model.userId }, new { @class = "edita" }) |
    @Html.ActionLink("Back to List", "DataUserAccount", new { @class = "bactolist" })
</p>
