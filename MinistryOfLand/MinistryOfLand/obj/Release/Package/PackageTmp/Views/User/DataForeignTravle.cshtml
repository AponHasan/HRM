@model IEnumerable<MinistryOfLand.Models.ForeignTravle>

@{
    ViewBag.Title = "DataForeignTravle";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Foreign Travle</h2>

<div class="clildren-table">
    <table class="table">
        <tr>
            <th>
                User Name
            </th>
            <th>
                Country
            </th>
            <th>
                Purpuse
            </th>
            <th>
                Travle From
            </th>
            <th>
                Travle To
            </th>            
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.UserAccount.username)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Country)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Purpuse)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TFrom)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TTo)
                </td>                
                <td>
                    @Html.ActionLink("Edit", "EditForeignTravle", new { id = item.ForeignTravelId }, new { @class = "edita" }) 
                    @Html.ActionLink("Details", "DetailsForeignTravleo", new { id = item.ForeignTravelId }, new { @class = "details" }) 
                    @Html.ActionLink("Delete", "DeleteForeignTravle", new { id = item.ForeignTravelId }, new { @class = "deletea" })
                </td>
            </tr>
        }

    </table>
</div>