@model MinistryOfLand.Models.ChildrenInfo

@{
    ViewBag.Title = "DetailsChildren";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>DetailsChildren</h2>

<div>
    <h4>ChildrenInfo</h4>
	<hr />
    <dl class="dl-horizontal">
        <dt>
            Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ChildrenName)
        </dd>

        <dt>
            Date Of Birth
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ChildrenDOB)
        </dd>

        <dt>
            Gender
